{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "allowJs": true,
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@utils": ["./src/utils/index.ts"],
      "@utils/*": ["./src/utils/*"],
      "@/components/*": ["./src/components/*"],
      "@styles/*": ["./src/styles/*"],
      "@/*": ["./src/*"]
    }
  }
}
